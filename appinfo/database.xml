<?xml version="1.0" encoding="ISO-8859-1" ?>
<database>

	<name>*dbname*</name>
	<create>true</create>
	<overwrite>false</overwrite>

	<charset>utf8</charset>

	<table>

		<name>*dbprefix*files_accounting</name>

		<declaration>

			<field>
				<name>fa_id</name>
				<type>integer</type>
				<default>0</default>
				<notnull>true</notnull>
				<autoincrement>1</autoincrement>
				<length>4</length>
			</field>

			<field>
				<name>user</name>
				<type>text</type>
				<notnull>true</notnull>
				<length>64</length>
			</field>

		        <field>
				<name>status</name>
				<type>boolean</type>
				<notnull>true</notnull>	
			</field>

			<field>
				<name>month</name>
                                <type>text</type>
                                <notnull>true</notnull>
                                <length>255</length>
			</field>

			<field>
                                <name>average</name>
                                <type>text</type>
                                <notnull>true</notnull>
                                <length>255</length>
                        </field>

			<field>
                                <name>trashbin</name>
                                <type>text</type>
                                <notnull>true</notnull>
                                <length>255</length>
                        </field>

			<field>
                                <name>bill</name>
                                <type>text</type>
                                <notnull>true</notnull>
                                <length>255</length>
                        </field>

			<field>
                                <name>invoice_link</name>
                                <type>text</type>
                                <notnull>true</notnull>
                                <length>255</length>
                        </field>

			 <field>
                                <name>reference_id</name>
                                <type>text</type>
                                <notnull>true</notnull>
                                <length>255</length>
                        </field>
			
			<field>
                                <name>year</name>
                                <type>text</type>
                                <notnull>true</notnull>
                                <length>255</length>
                        </field>

	</table>	

</database>

